{
  "$schema": "https://raw.githubusercontent.com/F5Networks/f5-declarative-onboarding/master/schema/latest/base.schema.json",
  "schemaVersion": "1.0.0",
  "class": "Device",
"async": true,
  "Common": {
    "class": "Tenant",
      "hostname": "{{ inventory_hostname }}",
      "myLicense": {
          "class": "License",
          "licenseType": "regKey",
          "overwrite": true,
          "regKey": "{{ f5_license }}"
      },
      "managementRoute": {
        "class": "ManagementRoute",
        "gw": "{{ net_mgmt.gw }}",
        "network": "default",
        "mtu": 1500,
        "type": "interface"
      },
      "myDns": {
          "class": "DNS",
          "nameServers": [
            "{{ vm_dns }}"
          ],
          "search": [
              "f5.com",
              "test.com"
          ]
      },
      "myNtp": {
          "class": "NTP",
          "servers": [
              "{{ vm_ntp }}"
          ],
          "timezone": "UTC"
      },
      "myProvisioning": {
        "class": "Provision",
        "ltm": "nominal"
      },
      "admin": {
          "class": "User",
          "userType": "regular",
          "password": "{{ vm_admin_pass }}",
          "shell": "bash"
      },
      "internal": {
          "class": "VLAN",
          "tag": 1001,
          "mtu": 1500,
          "interfaces": [
              {
                  "name": "1.1",
                  "tagged": false
              }
          ]
      },
      "internal-self": {
          "class": "SelfIp",
          "address": "{{ net_internal.ip }}/24",
          "vlan": "internal",
          "allowService": "all",
          "trafficGroup": "traffic-group-local-only"
      },
      "external": {
          "class": "VLAN",
          "tag": 1002,
          "mtu": 1500,
          "interfaces": [
              {
                  "name": "1.2",
                  "tagged": false
              }
          ]
      },
      "external-self": {
          "class": "SelfIp",
          "address": "{{ net_external.ip }}/24",
          "vlan": "external",
          "allowService": "all",
          "trafficGroup": "traffic-group-local-only"
      },
      "default": {
          "class": "Route",
          "gw": "{{ net_external.gw }}",
          "network": "default",
          "mtu": 1500
      },
      "ha": {
          "class": "VLAN",
          "tag": 1003,
          "mtu": 1500,
          "interfaces": [
              {
                  "name": "1.3",
                  "tagged": false
              }
          ]
      },
      "ha-self": {
          "class": "SelfIp",
          "address": "{{ net_ha.ip }}/24",
          "vlan": "internal",
          "allowService": "all",
          "trafficGroup": "traffic-group-local-only"
      },
      "configsync": {
        "class": "ConfigSync",
        "configsyncIp": "/Common/ha-self/address"
      },
      "failoverAddress": {
          "class": "FailoverUnicast",
          "address": "/Common/ha-self/address"
      },
      "failoverGroup": {
          "class": "DeviceGroup",
          "type": "sync-failover",
          "members": ["{{ bigip1 }}", "{{ bigip2 }}"],
          "owner": "{{ bigip1 }}",
          "autoSync": true,
          "saveOnAutoSync": false,
          "networkFailover": true,
          "fullLoadOnSync": false,
          "asmSync": false
      },
      "trust": {
          "class": "DeviceTrust",
          "localUsername": "admin",
          "localPassword": "{{ vm_admin_pass }}",
          "remoteHost": "{{ trust_peer_ip }}",
          "remoteUsername": "admin",
          "remotePassword": "{{ vm_admin_pass }}"
      }
  }
}